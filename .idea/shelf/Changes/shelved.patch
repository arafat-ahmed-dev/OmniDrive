onClick={(e) => handleRemoveFile(e, file.name)}\r\n                />\r\n              </li>\r\n            );\r\n          })}\r\n        </ul>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default FileUploader;\r\n
